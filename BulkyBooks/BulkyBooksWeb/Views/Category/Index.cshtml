@model Spolek
@{
    ViewData["Title"] = "Index";
}

<h1>Index Category</h1>

<h1>@Model.HlavniNazev</h1>

<table class="table table-bordered table-striped" style="width:100%">
    <thead>
        <tr>
            <th>
                Category name
            </th>
            <th>
                Displey Order
            </th>
        </tr>
    </thead>
    <tbody>
        @foreach(var obj in Model.ListKatergorii)
        {
            <tr>
                <td width="50%">
                    @obj.Name
                </td>
                <td width="30%">
                    @obj.DisplayOrder
                </td>
            </tr>
        }
    </tbody>
</table>

